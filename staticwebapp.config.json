{
  "routes": [
    {
      "route": "/logout.html",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/logout.html",
      "statusCode": 302
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "default-src 'self' https://login.microsoftonline.com https://kaizen-bba6guhvhhdxf2ba.swedencentral-01.azurewebsites.net; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; connect-src 'self' https://kaizen-bba6guhvhhdxf2ba.swedencentral-01.azurewebsites.net"
  }
}
